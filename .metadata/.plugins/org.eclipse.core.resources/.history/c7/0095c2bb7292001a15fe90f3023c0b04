http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<flow name="sandboxFlow" doc:id="5a05bf74-becb-4f1e-a4c9-2b86c4747b5e" >
		<scheduler doc:name="Scheduler" doc:id="5c340fd5-1aa4-4f47-820e-9102bf7dad75" >
			<scheduling-strategy >
				<fixed-frequency frequency="3000"/>
			</scheduling-strategy>
		</scheduler>
		<ee:transform doc:name="Transform Message" doc:id="64f7fcbd-8e3e-4b28-a05b-0b6d6a96e8bf" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/java
---
"object-%pod-" ++ now() ++ "-salt"]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<validation:matches-regex doc:name="Matches regex" doc:id="5d1b05f5-567a-4a18-8d16-df765b734868" value="object-200519-12:01:35:068-TZ-9" regex="#[/^[A-Za-z0-9:\-]*$/]" message="bad input!"/>
		<logger level="INFO" doc:name="Logger" doc:id="925f3e09-8340-453d-8c5b-9bcd3ff8935d" message="#[payload]"/>
		<error-handler >
			<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="b7276f31-a345-4e96-9833-40928905b0f7" >
				<set-payload value='#["\nMessage from error: " ++ (error.errorMessage as String)]' doc:name="Set Payload" doc:id="b14b0cd7-1216-4c14-8547-0508b81f5615" />
				<logger level="INFO" doc:name="Logger" doc:id="13e61a96-3990-463b-9d0f-631477c5e62a" message="#[payload]"/>
			</on-error-continue>
		</error-handler>
	</flow>
</mule>
